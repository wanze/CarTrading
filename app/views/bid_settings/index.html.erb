<h1>Listing bid_settings</h1>

<table>
  <thead>
    <tr>
      <th>Offer</th>
      <th>User</th>
      <th>Automatic</th>
      <th>Max amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bid_settings.each do |bid_setting| %>
      <tr>
        <td><%= bid_setting.offer_id %></td>
        <td><%= bid_setting.user_id %></td>
        <td><%= bid_setting.automatic %></td>
        <td><%= bid_setting.max_amount %></td>
        <td><%= link_to 'Show', bid_setting %></td>
        <td><%= link_to 'Edit', edit_bid_setting_path(bid_setting) %></td>
        <td><%= link_to 'Destroy', bid_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bid setting', new_bid_setting_path %>
